{
    "configurations": [
        {
            "name": "Linux",
            "includePath": [
                "${workspaceFolder}/**"
            ],
            "defines": [],
            "cStandard": "c17"
        },
        {
            "name": "i686-elf",
            "includePath": [
                "${workspaceFolder}/**",
                "${workspaceFolder}/src/bootloader/stage3/"
            ],
            "defines": [],
            "cStandard": "c17",
            "compilerPath": "${workspaceFolder}/toolchain/i686-elf/bin/i686-elf-g++",
            "compilerArgs": [
                "-c",
                "-ffreestanding",
                "-fno-exceptions",
                "-fno-asynchronous-unwind-tables",
                "-nostdinc",
                "-nostdlib",
                "-pedantic",
                "-Wall",
                "-Wextra",
                "-Wcast-align",
                "-Wcast-qual",
                "-Wctor-dtor-privacy",
                "-Wdisabled-optimization",
                "-Wformat=2",
                "-Winit-self",
                "-Wlogical-op",
                "-Wmissing-declarations",
                "-Wmissing-include-dirs",
                "-Wnoexcept",
                "-Wold-style-cast",
                "-Woverloaded-virtual",
                "-Wredundant-decls",
                "-Wshadow",
                "-Wsign-conversion",
                "-Wsign-promo",
                "-Wstrict-null-sentinel",
                "-Wstrict-overflow=0",
                "-Wswitch-default",
                "-Wundef",
                "-Werror",
                "-Wno-unused"
            ],
            "cppStandard": "c++20",
            "intelliSenseMode": "gcc-x86"
        }
    ],
    "version": 4
}